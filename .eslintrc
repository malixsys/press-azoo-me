{
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "console": true,
    "consoleLog": true,
    "Blob": true,
    "File": true,
    "FormData": true
  },
  "plugins": [
    "react",
    "auto-import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "max-len": ["error", {"code": 181}],
    "no-multiple-empty-lines": ["error", {"max": 1, "maxBOF": 1, "maxEOF": 0}],
    "comma-dangle": "error",
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "react/prop-types": "warn",
    "no-unused-vars": "warn",
    "no-console": "error",
    "semi": "warn",
    "no-undef": "error",
    "no-mixed-spaces-and-tabs": "warn",
    "no-useless-escape": "warn",
    "no-unexpected-multiline": "warn"
  }
}
